        "icons",
        "antlr",
        "parser"
    ],
    "author": "Sergey Vartanov",
    "license": "MIT",
    "devDependencies": {
        "prettier": "^3.0.0"
    },
    "dependencies": {
        "antlr4": "^4.13.0",
        "paper": "^0.12.17"
    }
}